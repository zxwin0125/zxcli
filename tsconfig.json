/* Visit https://aka.ms/tsconfig.json to read more about this file */
{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "lib": ["ES2020"],
    "sourceMap": true,
    "declaration": true,
    "removeComments": true,
    "outDir": "lib",
    "strict": true,
    "esModuleInterop": true,
    "pretty": true
  },
  "include": ["src/**/*.ts"], // declarations.d.ts
  "exclude": ["node_modules", "lib"] // 排除不编译的文件夹
}
